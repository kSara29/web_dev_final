@model List<Instagram.ViewModels.User.UserSearchResultVm>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Результаты поиска пользователей</h2>

@if (Model.Count == 0)
{
    <p>Пользователи не найдены.</p>
}
else
{
    <ul>
        @foreach (var user in Model)
        {
            <li>
                <a asp-controller="User" asp-action="Profile" asp-route-userId="@user.Id"> @user.UserName </a>
            </li>
        }
    </ul>
}
